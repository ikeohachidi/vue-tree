{
  "name": "vue-quick-tree",
  "description": "A simple and straightforward vue tree component",
  "version": "0.1.13",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "keywords": [
    "tree",
    "recursive",
    "hierachy",
    "quick"
  ],
  "files": ["dist"],
  "main": "./dist/vue-quick-tree.umd.js",
  "module": "./dist/vue-quick-tree.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-quick-tree.es.js",
      "require": "./dist/vue-quick-tree.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikeohachidi/vue-tree"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.10.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.0",
    "vue": "^3.0.5",
    "vue-tsc": "^0.0.24"
  }
}
