{
  "name": "vue-quick-tree",
  "description": "A simple and straightforward vue tree component",
  "version": "0.1.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "keywords": ["tree", "recursive", "hierachy", "quick"],
  "main": "./dist/vue-tree.umd.js",
  "module": "./dist/vue-tree.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-tree.es.js",
      "require": "./dist/vue-tree.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikeohachidi/vue-tree"
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.0",
    "vue-tsc": "^0.0.24"
  }
}
